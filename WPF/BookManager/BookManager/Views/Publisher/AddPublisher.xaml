<Window x:Class="BookManager.Views.Publisher.AddPublisher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookManager"
        mc:Ignorable="d"
        Title="AddAuthor" Height="300" Width="300">
    <Grid>
        <Button x:Name="btnInsert" Content="_Insert" HorizontalAlignment="Left" Margin="168,196,0,0" VerticalAlignment="Top" Width="75" Click="btnInsert_Click"/>
        <Label x:Name="label" Content="Publisher Name" HorizontalAlignment="Left" Margin="26,54,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtName" HorizontalAlignment="Left" Height="23" Margin="123,54,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnCancel" Content="_Cancel" HorizontalAlignment="Left" Margin="45,196,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
